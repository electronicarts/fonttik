{
  "name": "fonttik",
  "description": [ "Needed" ],
  "version-string": "1.0.3",
  "default-features": [ "default-opencv" ],
  "dependencies": [
    "nlohmann-json",
    "spscqueue",
    "gtest",
    "spdlog",
    "benchmark",
    {
      "name": "ffmpeg",
      "features": [ "openh264", "openjpeg", "zlib" ]
    }

  ],
  "features": {
    "cuda-opencv": {
      "description": "Uses opencv with CUDA capabilities",
      "dependencies": [
        {
          "name": "opencv",
          "features": [ "ffmpeg", "cuda", "cudnn", "dnn-cuda" ]
        },
        "cuda",
        "cudnn"
      ]
    },
    "default-opencv": {
      "description": "Uses opencv with default features",
      "dependencies": [
        {
          "name": "opencv",
          "features": [ "ffmpeg", "dnn" ]
        }
      ]
    }
  }
}